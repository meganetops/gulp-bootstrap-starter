<%
    pageTitle = "パーシャルループ"
%>
<!DOCTYPE html>
<html lang="ja">
<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# article: http://ogp.me/ns/article#">

    <meta charset="UTF-8">
    <title><%= theme.pageTitle %> - <%= theme.siteName %></title>

    <meta property="og:type" content="website">
    <meta property="og:locale" content="ja_JP">

    <meta property="og:site_name" content="<%= theme.siteName %>">
    <meta property="og:title" content="<%= theme.pageTitle %>">
    <meta property="og:description" content="これは<%= theme.pageTitle %>のサンプルです。">
    <meta property="og:url" content="<%= theme.siteRootURL %>index.html">
    <meta property="og:image" content="<%= theme.siteRootURL %>images/facebook.png">

    <meta property="fb:app_id" content="<%= theme.facebookAppID %>">

</head>
<body>
    
    <% shops.forEach(function(shop){ %>
    <% include ejs/loop %>
    <% }) %>

    <% if(theme.ga && theme.ga.length>1){ %>
    <% include ejs/ga %>
    <% } %>
</body>
</html>